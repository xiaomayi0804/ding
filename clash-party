proxy-groups:

  - name: 🔯 自选故转
    type: fallback
    interval: 300
    lazy: false
    proxies:
      - ♻️ Airport_02
      - ♻️ Airport_01
      - 所有-自动

  - {name: ♻️ Airport_01, type: url-test, tolerance: 20, interval: 300, use: [Airport_01]}
  - {name: ♻️ Airport_02, type: url-test, tolerance: 20, interval: 300, use: [Airport_02]}

  - name: 所有-手动
    type: select
    include-all: true
    filter: "^((?!(直连|拒绝)).)*$"

  - name: 所有-自动
